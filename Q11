def job_scheduling(jobs, n):
    jobs.sort(key=lambda x: x[2], reverse=True)
    result = [False] * n
    job_order = ['-1'] * n
    for job in jobs:
        for j in range(min(n, job[1]) - 1, -1, -1):
            if not result[j]:
                result[j] = True
                job_order[j] = job[0]
                break
    print("Job order:", job_order)

jobs = [
    ('a', 2, 100),
    ('b', 1, 19),
    ('c', 2, 27),
    ('d', 1, 25),
    ('e', 3, 15)
]

job_scheduling(jobs, 3)
